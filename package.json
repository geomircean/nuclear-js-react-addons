{
  "name": "@gm/nuclear-js-react-addons",
  "version": "0.4.1",
  "description": "React addons for nuclear-js",
  "main": "build/index.js",
  "scripts": {
    "make-build": "rm -rf build && babel ./src --out-dir build",
    "dist": "rm -rf build  && webpack src/index.js dist/NuclearJSReactAddons.js && webpack -p src/index.js dist/NuclearJSReactAddons.min.js",
    "build": "npm run make-build && npm run dist",
    "prepare": "npm run build",
    "test": "npm run lint && karma start --single-run",
    "lint": "eslint .",
    "watch": "karma start"
  },
  "authors": [
    "Jordan Garcia",
    "Serge Havas"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordangarcia/nuclear-js-react-addons/issues"
  },
  "homepage": "https://github.com/jordangarcia/nuclear-js-react-addons",
  "repository": {
    "type": "git",
    "url": "git://github.com/jordangarcia/nuclear-js-react-addons"
  },
  "keywords": [
    "flux",
    "nuclear",
    "immutable",
    "react",
    "addons",
    "functional",
    "stateless"
  ],
  "files": [
    "build",
    "dist"
  ],
  "dependencies": {
    "@babel/preset-react": "^7.22.15",
    "hoist-non-react-statics": "^1.0.2",
    "object-assign": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.22.20",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.33.2",
    "expect": "^1.13.4",
    "jsdom": "^6.1.0",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "mocha-jsdom": "^1.0.0",
    "node-libs-browser": "^0.5.2",
    "nuclear-js": "^1.2.1",
    "prop-types": "^15.8.1",
    "react": "^15.7.0",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^15.7.0",
    "should": "^7.0.4",
    "webpack": "^3.12.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^1.10.1"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  }
}
